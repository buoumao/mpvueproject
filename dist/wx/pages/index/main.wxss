
page {
  background: #8ed145;
}
.index-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.index-img {
  height: 200rpx;
  width: 200rpx;
  border-radius: 100rpx;
  margin: 100rpx 0;
}
.username {
  font-size: 40rpx;
  font-weight: bold;
  margin: 100rpx 0;
  color: #eee;
}
.go-study {
  width: 220rpx;
  height: 80rpx;
  border: 1rpx solid #eee;
  font-size: 24rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 10rpx;
}
.go-study ._p {
  color: #eee;
}
.btn {
  width: 300rpx;
  height: 300rpx;
  border-radius: 150rpx;
  margin: 50rpx 0;
  line-height: 300rpx;
  text-align: center;
}